			+e.item.speaker__
				+e.body
					+e.wrapper
						+e.name
							h3 Владимир Попов
							p Иркутск, Россия
						+e.socials.socials__.--speaker
							+e.A(href="#").item--fb
							+e.A(href="#").item--in
